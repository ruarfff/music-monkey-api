{
  "name": "music-monkey-api",
  "version": "1.0.0",
  "private": true,
  "main": "build/server.js",
  "scripts": {
    "lint": "tslint --fix -c tslint.json src/**/*.ts",
    "start": "node ./build/server/server.js",
    "stop": "node ./node_modules/.bin/pm2 stop pm2.json",
    "start:watch": "npm run start -- --watch",
    "flush-logs": "node ./node_modules/.bin/pm2 flush",
    "update-pm2": "node ./node_modules/.bin/pm2 update",
    "compile": "tsc",
    "compile:watch": "npm run compile -- -w",
    "clean": "rimraf build/*",
    "posttest": "npm run lint",
    "test": "mocha --recursive --reporter spec \"**/*.spec.js\"",
    "test:watch": "npm test -- --watch",
    "security-check": "snyk test"
  },
