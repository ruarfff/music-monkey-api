    "compile:watch": "npm run compile -- -w",
    "clean": "rimraf build/*",
    "posttest": "npm run lint",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "watch-test": "npm run test -- --watchAll",
    "security-check": "snyk test"
