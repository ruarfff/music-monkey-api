  "scripts": {
    "lint": "tslint --fix -c tslint.json src/**/*.ts",
    "start": "node ./build/server/server.js",
    "stop": "node ./node_modules/.bin/pm2 stop pm2.json",
    "start:watch": "npm run start -- --watch",
    "flush-logs": "node ./node_modules/.bin/pm2 flush",
    "update-pm2": "node ./node_modules/.bin/pm2 update",
