{
  "name": "music-monkey-api",
  "version": "1.0.0",
  "private": true,
  "main": "build/server/server.js",
  "scripts": {
    "lint": "tslint --fix -c tslint.json 'server/**/*.ts'",
    "start": "node ./node_modules/.bin/pm2 start pm2.json",
    "start:watch": "npm run start -- --watch",
    "poststart": "node ./node_modules/pm2/bin/pm2 logs",
    "compile": "copyfiles \"server/**/*.json\" build/server -u 1 && tsc",
    "compile:watch": "npm run compile -- -w",
    "clean": "rimraf build/*",
    "posttest": "npm run lint",
    "test": "mocha --recursive --reporter spec \"**/*.spec.js\"",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "aws-sdk": "2.232.1",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "dynamodb": "1.1.1",
    "helmet": "3.12.0",
    "joi": "13.2.0",
    "loopback": "3.19.0",
    "loopback-boot": "2.27.1",
    "loopback-component-explorer": "6.0.0",
    "multer": "1.3.0",
    "pm2": "2.10.3",
    "serve-favicon": "2.5.0",
    "spotify-web-api-node": "3.0.0",
    "strong-error-handler": "2.3.2",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "@types/es6-promise": "3.3.0",
    "@types/joi": "13.0.8",
    "@types/loopback": "3.2.3",
    "@types/loopback-boot": "2.23.4",
    "@types/node": "10.0.3",
    "chai": "4.1.2",
    "copyfiles": "2.0.0",
    "eslint": "4.19.1",
    "eslint-config-loopback": "10.0.0",
    "mocha": "5.1.1",
    "nsp": "3.2.1",
    "rimraf": "2.6.2",
    "tslint": "5.10.0",
    "typescript": "2.9.0-dev.20180506"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "music-monkey-api"
}
